    "@forgestack/db": "workspace:*",
    "@forgestack/shared": "workspace:*",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/swagger": "^11.2.3",
    "@opentelemetry/api": "^1.9.0",
