{
  "name": "@forgestack/ui",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.js",
      "require": "./dist/components/*.js"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "echo 'No tests configured yet'",
    "clean": "rm -rf dist",
    "storybook": "echo 'Storybook - implement after Storybook setup'",
    "build-storybook": "echo 'Storybook build - implement after Storybook setup'"
  },
  "dependencies": {
    "@forgestack/shared": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  }
}

